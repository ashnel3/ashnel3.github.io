{
  "endOfLine": "auto",
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
  "overrides": [
    {
      "files": ["*.astro"],
      "options": { "parser": "astro" }
    },
    {
      "files": ["*.svelte"],
      "options": { "parser": "svelte" }
    }
  ]
}
